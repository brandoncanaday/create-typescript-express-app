{
    "name": "create-typescript-express-app",
    "version": "0.0.0",
    "description": "Simple API for GameChanger interview",
    "author": "Brandon Canaday",
    "scripts": {
        "start": "node dist/server.js",
        "start:local": "env-cmd ts-node-dev --respawn --transpile-only src/server.ts",
        "clean": "rimraf dist/",
        "build": "yarn run clean && tsc",
        "lint-es": "eslint --ext .js,.jsx,.ts,.tsx --format=pretty .",
        "lint-md": "markdownlint '**/*.md'",
        "lint": "run-p lint-es lint-md",
        "test": "jest --ci --coverage --silent",
        "commit": "cz",
        "release": "semantic-release --no-verify",
        "docker:lint": "docker build --progress=plain --target lint . -t $npm_package_name-lint --build-arg PROJECT_NAME=$npm_package_name",
        "docker:test": "docker build --progress=plain --target test . -t $npm_package_name-test --build-arg PROJECT_NAME=$npm_package_name",
        "docker:build": "docker build --progress=plain --target build . -t $npm_package_name-build --build-arg PROJECT_NAME=$npm_package_name",
        "docker:validate": "docker build --progress=plain --target validate . -t $npm_package_name-validate --build-arg PROJECT_NAME=$npm_package_name",
        "docker:start-local:build": "docker build --progress=plain --target start-local . -t $npm_package_name-start-local --build-arg PROJECT_NAME=$npm_package_name",
        "docker:start-local:stop": "docker stop $npm_package_name || true && docker rm $npm_package_name || true",
        "docker:start-local:run": "docker run --name $npm_package_name -p 5000:5000 -v $(pwd)/src:/usr/src/$npm_package_name/src $npm_package_name-start-local",
        "docker:start-local": "run-s docker:start-local:build docker:start-local:stop docker:start-local:run",
        "docker:final": "docker build --progress=plain . -t $npm_package_name:$npm_package_version --build-arg PROJECT_NAME=$npm_package_name"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "prepare-commit-msg": "exec < /dev/tty && git-cz --hook || true",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-push": "yarn run docker:validate"
        }
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "debug": "^4.3.1",
        "express": "^4.17.1",
        "express-async-handler": "^1.1.4",
        "helmet": "^4.6.0",
        "http-errors": "^1.8.0",
        "http-terminator": "^3.0.0",
        "moment-timezone": "^0.5.33",
        "morgan": "^1.10.0",
        "node-fetch": "^2.6.1",
        "serialize-error": "^8.1.0",
        "swagger-ui-express": "^4.1.6",
        "tslib": "^2.3.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "@semantic-release/changelog": "^5.0.1",
        "@semantic-release/commit-analyzer": "^8.0.1",
        "@semantic-release/exec": "^5.0.0",
        "@semantic-release/git": "^9.0.0",
        "@semantic-release/npm": "^7.1.3",
        "@semantic-release/release-notes-generator": "^9.0.3",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.10",
        "@types/debug": "^4.1.5",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.12",
        "@types/helmet": "^4.0.0",
        "@types/http-errors": "^1.8.0",
        "@types/http-terminator": "^2.0.1",
        "@types/jest": "^26.0.23",
        "@types/morgan": "^1.9.2",
        "@types/node": "^15.12.5",
        "@types/node-fetch": "^2.5.10",
        "@types/supertest": "^2.0.11",
        "@types/swagger-ui-express": "^4.1.2",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.28.1",
        "@typescript-eslint/parser": "^4.28.1",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "dotenv": "^10.0.0",
        "env-cmd": "^10.1.0",
        "eslint": "^7.29.0",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-formatter-pretty": "^4.1.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jest": "^24.3.6",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-unicorn": "^34.0.1",
        "husky": "^6.0.0",
        "jest": "^27.0.6",
        "jest-junit": "^12.2.0",
        "lint-staged": "^11.0.0",
        "markdownlint-cli": "^0.27.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "semantic-release": "^19.0.3",
        "supertest": "^6.1.3",
        "ts-jest": "^27.0.3",
        "ts-node-dev": "^1.1.7",
        "typescript": "^4.3.4"
    }
}
